[run]
branch = True
omit = 
       *gen/jaeger*
       *gen/opencensus*
       opencensus/__init__.py
       opencensus/common/__init__.py

[report]
fail_under = 100
show_missing = True
exclude_lines =
    # Re-enable the standard pragma
    pragma: NO COVER
    # Ignore debug-only repr
    def __repr__
omit = 
       *gen/jaeger*
       *gen/opencensus*
       opencensus/__init__.py
       opencensus/common/__init__.py
